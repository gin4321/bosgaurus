					<div class="fl-ns wi-60-l wi-55-m">
						<div class="bb-ns b--light-silver pb15">
							<div class="dib dib-fix v-mid wi-60-ns">
								<h2 class="f3 mv0 book lh-title">Shopping Cart</h2>
							</div>
							<div class="dn dib-ns dib-fix v-mid wi-25 f6 gray">
								Quantity
							</div>
							<div class="dn dib-ns dib-fix v-mid wi-15 f6 gray tr">
								Price
							</div>
						</div>
						{% for item in cart.items %}
						<div class="pv30 bb bb b--light-silver spec-cart-item">
							<div class="db dib-ns dib-fix v-mid wi-60-ns mb20 mb0-ns">
								<div class="dt">
									<div class="dtc v-mid wi-25 wi-33-ns">
										<img width="105" class="v-top" src="{{ item.image }}" alt="">
									</div>
									<div class="dtc pr20 wi-5">
									</div>
									<div class="dtc v-mid pr25 wi-75 wi-67-ns">
										<div class="f5">
											<a class="gray-text-link" href={{item.url}}>{{ item.title }}</a>
										</div>
										<div class="f7 serif-font italic">
											{{ item.description }}
										</div>
										<div class="mt5">
											<a rel="nofollow" href="/cart/change?line={{ forloop.index }}&quantity=0">Remove item</a>
										</div>
										<div class="pt5">
										</div>
									</div>
								</div>
							</div>
							<div class="dib dib-fix v-mid wi-25">
								<div class="quantity-input js-quantity-input spec-quantity-input pos-rel" data-dropdown-max="10" data-quantity="1" data-selected-variant-id="5284" data-updateable="true" data-variants="{&quot;5284&quot;:{&quot;dropdown_options&quot;:[{&quot;value&quot;:1,&quot;text&quot;:1},{&quot;value&quot;:2,&quot;text&quot;:2},{&quot;value&quot;:3,&quot;text&quot;:3},{&quot;value&quot;:4,&quot;text&quot;:4},{&quot;value&quot;:5,&quot;text&quot;:5},{&quot;value&quot;:6,&quot;text&quot;:6},{&quot;value&quot;:7,&quot;text&quot;:7},{&quot;value&quot;:8,&quot;text&quot;:8},{&quot;value&quot;:9,&quot;text&quot;:9},{&quot;text&quot;:&quot;10+&quot;,&quot;value&quot;:10}],&quot;purchaseable_limit&quot;:144,&quot;purchaseable_limit_is_dropdown_max&quot;:false}}">
									<div class="quantity-dropdown js-quantity-dropdown styled fancy-select">
										<div class="styled-select-box">
											<select class="quantity-select" name="id" value="{{ item.quantity }}">
												{% for i in (1..10) %}
													{% if i == item.quantity %}
														<option selected value="{{ i }}">{{ i }}</option>
													{% else %}
														<option value="{{ i }}">{{ i }}</option>
													{% endif %}
												{% endfor %}
											</select>
										</div>
									</div>
								</div>
							</div>
							<div class="dib dib-fix v-mid wi-75 wi-15-ns tr">
								<p class="f5">
									<span class="db-ns bold js-variant-price">{{ item.price | times: item.quantity | money }}</span>
								</p>
							</div>
						</div>
						{% endfor %}
						<p class="mt25 mb0">
							<a class="clear-cart f5 blue-text-link" rel="nofollow">Empty your cart</a>
						</p>
					</div>
        </div>
    </div>
